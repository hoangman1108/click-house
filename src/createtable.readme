
CREATE TABLE SM.users
(
`UserId` UInt32,
`Account` String,
`Telephone` String,
`Name` String,
`Title` String,
`Company` String,
`Department` String,
`RegisterTime` String,
`Role` String,
`Region` String,
`Preference` String,
`UserType` String,
`RoleExpireTime` String,
`UpdateTime` String,
`Expert` String,
`Status` String,
PRIMARY KEY(UserId)
)

ENGINE = MergeTree()
