CREATE TABLE SM.users
(
`UserId` UInt32,
`Account` String,
`Telephone` String,
`Name` String,
`Title` String,
`Company` String,
`Department` String,
`RegisterTime` DateTime('Europe/London'),
`Role` String,
`Region` String,
`Preference` String,
`UserType` String,
`RoleExpireTime` Date32,
`UpdateTime` DateTime('Europe/London'),
`Expert` String,
`Status` String,
PRIMARY KEY(UserId)
)

ENGINE = MergeTree()


